.authon{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-right: 60rpx;
}
.gxlIco{
  margin-left: 10rpx;
  width: 30rpx;
  height: 30rpx;
}

.checkUserLogin{
  width: 600rpx;
  height: 300rpx;
  background: #fff;
  border-radius: 10rpx;
  animation: show .3s ease;
}
@keyframes show{
  from{
    opacity: 0;
  }
  to{
    opacity: 1;
  }
}