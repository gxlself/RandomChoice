.pages{
  background: #eee;
  height: 100%;
}
.outBox{
  margin-bottom: 20rpx;
}
.outBox .innerBox:nth-of-type(2){
  border-top: none;
}
.innerBox{
  position: relative;
  border-top: 1rpx solid #ddd;
  border-bottom: 1rpx solid #ddd;
  padding: 15rpx 15rpx 15rpx 25rpx;
  background: #fff;
}
.innerBox::after{
  position: absolute;
  content: '>';
  right: 24rpx;
  color: #eee;
  transform: translateY(-3rpx);
}
